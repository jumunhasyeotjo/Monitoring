{
  "annotations": { "list": [] },
  "editable": true,
  "panels": [
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "custom": { "displayMode": "color-background" },
          "mappings": [
            { "type": "value", "options": { "0": { "text": "üî¥ DOWN", "color": "red" }, "1": { "text": "üü¢ UP", "color": "green" } } }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [ { "color": "red", "value": null }, { "color": "green", "value": 1 } ]
          }
        }
      },
      "gridPos": { "h": 12, "w": 6, "x": 0, "y": 0 },
      "id": 1,
      "targets": [
        {
          "expr": "up{job=~\"jumunhasyeo.*\"}",
          "format": "table",
          "instant": true,
          "legendFormat": "{{container_name}}",
          "refId": "A"
        }
      ],
      "title": "üö¶ Service Health Status",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true, "__name__": true, "instance": true, "job": true, "cluster_arn": true, "task_arn": true, "task_group": true, "task_revision": true, "docker_image": true, "container_arn": true },
            "indexByName": { "container_name": 0, "Value": 1 },
            "renameByName": { "container_name": "Service Name", "Value": "Status" }
          }
        }
      ],
      "type": "table"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": { "defaults": { "unit": "rps" } },
      "gridPos": { "h": 6, "w": 18, "x": 6, "y": 0 },
      "id": 2,
      "targets": [
        { "expr": "sum(rate(http_server_requests_seconds_count[1m]))", "legendFormat": "Total TPS" }
      ],
      "title": "üìà Total System Traffic (TPS)",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": { "defaults": { "unit": "percent", "max": 100 } },
      "gridPos": { "h": 6, "w": 9, "x": 6, "y": 6 },
      "id": 3,
      "targets": [
        { "expr": "(sum(rate(http_server_requests_seconds_count{status=~\"5..\"}[1m])) / sum(rate(http_server_requests_seconds_count[1m]))) * 100" }
      ],
      "title": "üö® Global Error Rate (%)",
      "type": "gauge"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": { "defaults": { "unit": "ms" } },
      "gridPos": { "h": 6, "w": 9, "x": 15, "y": 6 },
      "id": 4,
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket[1m])) by (le)) * 1000" }
      ],
      "title": "‚è±Ô∏è Global P95 Latency",
      "type": "stat"
    }
  ],
